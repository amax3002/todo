<label for="task_name">Name</label>
<input id="task_name" type="text" name="task[name]" value="<%= @task.name %>">

<label for="task_due_date">Due Date</label>
<input id="task_due_date" type="date" name="task[due_date]" value="<%= @task.due_date %>">

<label for="task_complete">Complete</label>
<input id="task_complete" type="text" name="task[complete]" value="<%= @task.complete %>">

<input type="submit" value="Save">
